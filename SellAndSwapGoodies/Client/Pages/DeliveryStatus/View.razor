@page "/deliverystatuses/view/{id:int}"
@inject HttpClient _client
@inject NavigationManager _navManager
<h3>View</h3>
<FormComponent deliverystatus="deliverystatus" Disabled="true" />
@code {
	[Parameter] public int id { get; set; }
	DeliveryStatus? deliverystatus = new DeliveryStatus();
	protected async override Task OnParametersSetAsync()
	{
		deliverystatus = await
	_client.GetFromJsonAsync<DeliveryStatus>($"{Endpoints.DeliveryStatusesEndpoint}/{id}");
	}
}